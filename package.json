{
  "name": "express-es6-sample",
  "version": "0.0.0",
  "engines": {
    "node": "12.16.x",
    "npm": "6.x"
  },
  "private": true,
  "scripts": {
    "start": "npm run server",
    "server": "babel-node ./src/bin/www",
    "dev": "SET NODE_ENV=development npm run watch ./src/bin/www",
    "prod": "npm run build && SET NODE_ENV=production node ./dist/bin/www",
    "build": "npm run clean && babel ./src --config-file ./.babelrc --out-dir dist",
    "clean": "rimraf dist",
    "watch": "nodemon --config ./nodemon.json --"
  },
  "proxy": "http://localhost:5003",
  "dependencies": {
    "babel-node": "0.0.1-security",
    "bcrypt": "^4.0.1",
    "bigint-hash": "^0.2.2",
    "body-parser": "^1.19.0",
    "compass": "^0.1.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.0",
    "jsonwebtoken": "^8.5.1",
    "mercadopago": "^1.3.2",
    "moment": "^2.26.0",
    "mongoose": "^5.9.16",
    "mongoose-double": "0.0.1",
    "mongoose-float": "^1.0.4",
    "morgan": "~1.9.0",
    "nodemailer": "^6.4.10",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "rimraf": "^2.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.20.2",
    "nodemon": "^1.19.4"
  }
}
